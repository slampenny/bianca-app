[general]
enabled=yes
bindaddr=0.0.0.0
bindport=8088
prefix=

; Enable TLS/HTTPS if needed
;tlsenable=yes
;tlsbindaddr=0.0.0.0:8089
;tlscertfile=/etc/asterisk/keys/asterisk.pem
;tlsprivatekey=/etc/asterisk/keys/asterisk.pem

; Session handling
sessionlimit=100
session_inactivity=30000
session_keep_alive=60000